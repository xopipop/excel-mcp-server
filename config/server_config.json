{
  "excel": {
    "visible": false,
    "display_alerts": false,
    "enable_events": true,
    "calculation": "automatic"
  },
  "logging": {
    "level": "INFO",
    "file": "excel_mcp.log",
    "rotation": "10 MB",
    "retention": "7 days"
  },
  "cache": {
    "enabled": true,
    "workbooks_ttl": 600,
    "worksheets_ttl": 300,
    "range_ttl": 60,
    "max_size": 100
  },
  "performance": {
    "batch_operations": true,
    "auto_save": false,
    "auto_close": false
  },
  "security": {
    "trust_access_to_visual_basic_project": true,
    "enable_macros": true
  }
} 